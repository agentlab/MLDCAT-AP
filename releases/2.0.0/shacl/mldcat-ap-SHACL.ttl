mldcatap2:AgentShape a shacl:NodeShape;
  shacl:closed false;
  shacl:property mldcatap2:AgentShape.name;
  shacl:targetClass foaf:Agent .

mldcatap2:AgentShape.name rdfs:seeAlso "https://semiceu.github.io/MLDCAT-AP/releases/2.0.0#Agent.name";
  shacl:path foaf:name;
  shacl:name "name"@en;
  shacl:description "A name of the agent."@en;
  shacl:nodeKind shacl:Literal;
  shacl:minCount 1 .


mldcatap2:AlgorithmShape a shacl:NodeShape;
  shacl:closed false;
  shacl:property mldcatap2:AlgorithmShape.hasalgorithmtype,
    mldcatap2:AlgorithmShape.haslearningmethodtype;
  shacl:targetClass mls:Algorithm .

mldcatap2:AlgorithmShape.hasalgorithmtype rdfs:seeAlso "https://semiceu.github.io/MLDCAT-AP/releases/2.0.0#Algorithm.hasalgorithmtype";
  shacl:name "has algorithm type"@en;
  shacl:description "A relation between an algorithm the category of algorithms that belongs to."@en;
  shacl:path mlso:hasAlgorithmType;
  #shacl:nodeKind shacl:BlankNodeOrIRI;
  shacl:class skos:Concept .

mldcatap2:AlgorithmShape.haslearningmethodtype rdfs:seeAlso "https://semiceu.github.io/MLDCAT-AP/releases/2.0.0#Algorithm.haslearningmethodtype";
  shacl:name "has learning method type"@en;
  shacl:description "A relation between an algorithm learning method and the category of algorithm learning methods that it belongs to."@en;
  shacl:path mlso:hasLearningMethodType;
  #shacl:nodeKind shacl:BlankNodeOrIRI;
  shacl:class skos:Concept;
  shacl:maxCount 1 .


